{
  "name": "lookahead-server",
  "version": "1.0.0",
  "description": "UoM Timetable Optimiser",
  "main": "server.ts",
  "type": "module",
  "engines": {
    "node": "^14.18.2"
  },
  "scripts": {
    "build": "yarn tsc",
    "start": "node --loader ts-node/esm --tls-min-v1.0 server.ts",
    "express-debug": "node --inspect=5000 -r ts-node/register server.ts",
    "express-debug:watch": "yarn nodemon",
    "dev": "yarn nodemon --exec node --loader ts-node/esm server.ts",
    "prod": "node dist/server.js",
    "test": "yarn run build && mocha dist/test --recursive"
  },
  "dependencies": {
    "@sentry/node": "^6.16.1",
    "aws-sdk": "^2.1049.0",
    "body-parser": "^1.19.1",
    "cheerio": "1.0.0-rc.10",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "googleapis": "^92.0.0",
    "got": "^12.5.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "node-html-parser": "^6.1.4",
    "s3-streamlogger": "^1.7.0",
    "totp-generator": "^0.0.14",
    "tough-cookie": "^4.1.2",
    "winston": "^3.3.3",
    "typescript": "^4.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.2",
    "@types/totp-generator": "^0.0.4",
    "@types/morgan": "^1.9.3",
    "@types/winston": "^2.4.4",
    "chai": "^4.3.4",
    "jest": "^24.9.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.20",
    "prettier": "2.5.1",
    "ts-node": "^10.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trontor/lookahead.git"
  },
  "author": "Rohyl Joshi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Trontor/lookahead/issues"
  },
  "homepage": "https://github.com/Trontor/lookahead#readme",
  "proxy": "http://localhost:5000/"
}
